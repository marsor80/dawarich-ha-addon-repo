{
  "name": "Dawarich",
  "version": "1.0.2",
  "slug": "dawarich",
  "description": "Self-hosted location history web app",
  "arch": ["amd64", "aarch64", "armv7", "armhf"],
  "startup": "services",
  "boot": "auto",
  "ports": {"3000/tcp": 3000},
  "host_network": false,
  "map": ["data:rw"],
  "options": {
    "DB_PATH": "/data/dawarich.sqlite3",
    "PORT": 3000,
    "LOG_LEVEL": "info",
    "SECRET_KEY_BASE": ""
  },
  "schema": {
    "DB_PATH": "str",
    "PORT": "int",
    "LOG_LEVEL": "list(debug|info|warn|error)",
    "SECRET_KEY_BASE": "password"
  },
  "image": "ghcr.io/freika/dawarich:latest",
  "webui": "[PROTO:INGRESS]",
  "ingress": true,
  "ingress_port": 3000,
  "ingress_stream": true
}